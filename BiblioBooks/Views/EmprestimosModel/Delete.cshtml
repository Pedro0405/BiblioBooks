@model BiblioBooks.Models.EmprestimosModel

@{
    ViewData["Title"] = "Delete";
}
<div class="container bg-dark p-5 rounded-5">


<h1 class="text-light"> Tem certeza que deseja deletar isso?</h1>
<div>
    <hr />
    <dl class="row">
        <dt style="color:white" class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Recebedor)
        </dt>
        <dd style="color:white" class = "col-sm-10">
            @Html.DisplayFor(model => model.Recebedor)
        </dd>
        <dt style="color:white" class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Fornecedor)
        </dt>
        <dd style="color:white" class = "col-sm-10">
            @Html.DisplayFor(model => model.Fornecedor)
        </dd>
        <dt style="color:white" class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LivroEmprestado)
        </dt>
        <dd style="color:white" class = "col-sm-10">
            @Html.DisplayFor(model => model.LivroEmprestado)
        </dd>
        <dt style="color:white" class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataultimaAtualizacao)
        </dt>
        <dd style="color:white" class = "col-sm-10">
            @Html.DisplayFor(model => model.DataultimaAtualizacao)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="id" />
        <input type="submit" value="Deletar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-warning">Desistir</a>
    </form>
</div>
</div>